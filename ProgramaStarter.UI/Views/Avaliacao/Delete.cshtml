@model ProgramaStarter.Application.DTO.AvaliacaoDTO

<h3>Deletar Avaliação</h3>

<h4>Tem certeza que deseja deletar esta avaliação de @Html.DisplayFor(model => model.Starter.Nome)?</h4>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nota)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nota)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Projeto.Etapa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Projeto.Etapa)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Projeto.Modulo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Projeto.Modulo)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" asp-route-starterId="@Model.Starter.Id"/> |
        <button type="button" class="btn btn-outline-secondary"><a asp-action="Index" asp-route-starterId="@Model.Starter.Id">Retornar</a></button>
    </form>
</div>